<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.appshat.kherokhata">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.SET_ALARM" />

    <application
        android:name=".OldAcrivity.Fmcg"
        android:allowBackup="true"
        android:icon="@mipmap/fmcgmipmap"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/fmcgmipmap"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name=".NewUIActivity.UpdateTransaction"/>
        <activity android:name=".Notifications.NotificationsActivity" />
        <activity android:name=".NewUIActivity.TestUI" />
        <activity android:name=".NewUIActivity.Onboarding_Activity" />
        <activity
            android:name=".OldAcrivity.SplashScreen"
            android:theme="@style/noactionbar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OldAcrivity.RegActivity" />
        <activity android:name=".Kotlin.ui.ChatBot" />
        <activity
            android:name=".OldAcrivity.HomeActivity"
            android:screenOrientation="portrait" />

        <receiver android:name=".Notifications.AlarmBroadcast" />

        <activity android:name=".OldAcrivity.MapActivity" />
        <activity
            android:name=".OldAcrivity.MainActivity"
            android:theme="@style/noactionbar" />

        <meta-data
            android:name="com.google.android.gms.ads.AD_MANAGER_APP"
            android:value="true" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>